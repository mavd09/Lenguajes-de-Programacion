  punto b;
fin_estructura

funcion punto calcularalgo( ) hacer
  si( a == -1 ) entonces retornar 5; fin_si
  hacer
    mientras( no_acabe ) hacer
      hacer
        hacer
          hacer
            para( punto j = 0; j < 5 || j > 10; 1*1%4-1 ) hacer
              imprimir( calcularalgo( ), "asdasda" );
            fin_para
            mientras( hola > 0 ) hacer
              hola = hola+1;
            fin_mientras
          mientras( 1 );
          retornar 2;
        mientras( 1 );
      mientras( 1 );
    fin_mientras
    romper;
  mientras( acabe );

  seleccionar( a ) entre
    defecto:
  fin_seleccionar

fin_funcion

funcion_principal
  entero a = 3;
